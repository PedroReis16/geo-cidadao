{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UseSerilog": true,
  "BasePath": "feed-service",
  "ConnectionStrings": {
    "GeoDb": "Host=localhost;Database=geodb;Username=geo;Password=geo",
    "GeoLogsDb": "mongodb://localhost:27018/geocidadaoFeedServiceLogs",
    "Redis": "redis:6379"
  },
  "Services": {
    "GerenciamentoUsuariosAPI": "http://gerenciamento-usuarios-api:8080",
    "EngagementServiceAPI": "http://engagement-service-api:8080"
  },
  "AllowedHosts": "*",
  "ElasticSearch": {
    "Uri": "http://elasticsearch:9200",
    "DefaultIndex": "posts",
    "UserName": "",
    "Password": ""
  },
  "OAuth": {
    "Authority": "http://localhost:8082/realms/geocidadao",
    "Audience": "",
    "ClaimRoles": {
    },
    "GroupClaims": {
      "Group.Users": "/users",
      "Group.Managers": "/managers",
      "Group.Moderators": "/moderators"
    }
  },
  "ResponseCaching": {
    "MaxAge": 60
  },
  "InMemoryCaching": {
    "MaxAge": 3600
  },
  "MaxConcurrency": 0,
  "RabbitMQ": {
    "HostName": "localhost",
    "Username": "admin",
    "Password": "admin"
  }
}
